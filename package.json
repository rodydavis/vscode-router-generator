{
	"name": "router-generator",
	"displayName": "router-generator",
	"description": "Generate a routed based on file based routing and nested layouts.",
	"version": "0.0.1",
	"repository": "https://github.com/rodydavis/vscode-router-generator",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:router-generator.generate-json",
		"onCommand:router-generator.generate-flutter",
		"onCommand:router-generator.generate-react",
		"onCommand:router-generator.generate-lit",
		"onCommand:router-generator.generate-lit-ssr",
		"onCommand:router-generator.generate-lit-router-component"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "router-generator.generate-react",
				"title": "Generate React Router"
			},
			{
				"command": "router-generator.generate-json",
				"title": "Generate JSON for Routes"
			},
			{
				"command": "router-generator.generate-lit",
				"title": "Generate Lit Router"
			},
			{
				"command": "router-generator.generate-lit-ssr",
				"title": "Generate Lit SSR Server"
			},
			{
				"command": "router-generator.generate-lit-router-component",
				"title": "Generate Lit router component"
			},
			{
				"command": "router-generator.generate-flutter",
				"title": "Generate Flutter Router"
			}
		]
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/mocha": "^9.0.0",
		"@types/vscode": "^1.63.0",
		"@types/webpack-env": "^1.16.3",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-web": "^0.0.15",
		"assert": "^2.0.0",
		"eslint": "^8.6.0",
		"mocha": "^9.1.3",
		"process": "^0.11.10",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"vsce": "^2.6.3",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"buffer": "^6.0.3"
	}
}